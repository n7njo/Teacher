{
  "name": "skillforge-learning-platform",
  "version": "1.0.0",
  "description": "SkillForge - Docker-based learning platform with category→topic→lesson structure",
  "main": "index.js",
  "scripts": {
    "dev": "docker-compose up --build",
    "dev:backend": "docker-compose up --build backend",
    "dev:frontend": "docker-compose up --build frontend",
    "build": "docker-compose build",
    "test": "docker-compose run --rm backend npm test",
    "clean": "docker-compose down -v"
  },
  "keywords": [
    "learning",
    "education",
    "docker",
    "typescript",
    "react",
    "node"
  ],
  "author": "Claude Code",
  "license": "MIT",
  "dependencies": {
    "pg": "^8.11.0",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "typescript": "^5.0.0"
  }
}
